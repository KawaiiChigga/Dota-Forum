<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="dota"/>
  <table-filter match-name="post"/>
  <table-filter match-name="categories"/>
  <table-filter match-name="comment"/>
  <table-filter match-name="message"/>
  <table-filter match-name="dislikes"/>
  <table-filter match-name="likes"/>
  <table-filter match-name="reply"/>
  <table-filter match-name="report"/>
  <table-filter match-name="user"/>
</hibernate-reverse-engineering>
